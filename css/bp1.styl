
header
	width 100%
	height 680px
	overflow hidden
	.botonera
		height auto
		overflow hidden
		padding 20px 20px 0 10px 
		position relative
		white-space nowrap
		width 100%
		z-index 15
		ul
			text-align center
			.dia 
				border-radius 50%
				display inline-block
				height 65px 
				margin 0
				position relative
				width 55px
				p
					border-radius 50%
					color rgba(white,1)
					font-style italic
					font-weight 900
					height 35px
					left 25%
					line-height 1.6em
					position absolute
					text-align center
					top 17px
					width 35px 
					&:hover
						background rgba(cielo,1)
						color rgba(black,.7)
						font-weight 400
				.centrarMes
					margin 0 0 0 .35em
				.nomDia
					color rgba(cielo,1)
					font-size .7em
					font-weight 400
					left 35%
					position absolute
					top -5px
				&:last-child
					margin 0 2em 0 0
	.calendari
		color white
		display block
		height 445px
		margin 0 auto
		padding 3em 0 0 0 
		position relative
		width 100%
		z-index 10
		.dia
			font-size 1em
			font-weight 300
			text-align center
			text-shadow 0 1px 3px rgba(black,.4)
			animation animacionDia 3s 1
			-webkit-animation animacionDia 3s 1
		.numero
			font-size 4em
			font-weight 100
			margin -.1em 0
			text-align center
			text-shadow 0 1px 3px rgba(black,.4)
			animation animacionNumero 3s 1
			-webkit-animation animacionNumero 3s 1
		.mes
			font-size 1em
			font-weight 300
			text-align center
			text-shadow 0 1px 3px rgba(black,.4)
			animation animacionMes 3s 1
			-webkit-animation animacionMes 3s 1
	.eventActual
		color white
		display block
		font-size .9em
		font-weight 100
		height auto
		padding .7em 1em 1.2em 1em
		position relative
		z-index 5
		.contingutEventActual
			animation animacionLlista 3s 1
			-webkit-animation animacionLlista 3s 1
			.event
				color rgba(cielo,.65) 
				display block
				font-size .9em
				font-weight 400
				letter-spacing .055em
				line-height 1.2em
				margin .7em auto 0 auto
				text-align center
				width 90%
				.lloc
					color rgba(gray,1)
					font-size .7em
					letter-spacing 1px
					margin .5em 0 0 0 
					text-transform uppercase
					p
						&:before
							content '\e600'
							font-family icono
							margin 0 5px 0 0 
			.hora
				color rgba(cielo,.65)
				display block
				font-size .9em
				font-weight 900
				font-style italic
				margin .6em auto 0 auto
				text-align center
				width 200px
		.titol
			border-radius 10px
			color rgba(cielo,1)
			font-size 1em
			font-weight 400
			left 10px
			padding 0 .7em
			position absolute
			top 20px
			span
				position absolute
				left 50px
				top 0px
				transform rotateZ(180deg)
				-ms-transform rotateZ(180deg)
				-webkit-transform rotateZ(180deg)
				&:after
					content '\e603'
					font-family icono
section
	display block
	margin 0 0 60px 0
	overflow-x hidden
	position relative
	.llistaVisible
		-webkit-animation animacionLlista .3s 1
		animation animacionLlista .3s 1
		padding .8em 0 1em 0
		.event
			margin 20px 10px
			.detallEvent
				background rgba(white,.2)
				border-radius 15px 15px 0 0 
				display block
				height 30px
				margin .5em 0 0 0 
				position relative
				z-index 1
				.quan
					.data
						p
							color rgba(cielo,.5)
							font-size .70em
							font-weight 300
							left 43%
							position relative
							top -10px
					.hora
						color rgba(white,.8)
						font-size .65em
						font-style italic
						font-weight 900
						left 20px
						letter-spacing 0px
						position relative
						top 7px
						p
							&:after
								content 'h'
			.Event
				background rgba(white,.3)
				color rgba(cielo,1)
				display block
				font-size .9em
				font-weight 300
				line-height 1.3em
				padding 15px 25px 32px 20px
				position relative
				z-index 2
				p
					font-weight bolder
				.detall
					color rgba(cielo,1)
					display none
					font-size .8em
					font-weight 300
					line-height 1.3em
					margin 10px 20px 20px 0 
					width 102%
					&:after
						color rgba(black,.2)
						content '\e60b'
						font-family icono
						font-size 1em
				.lloc
					color rgba(cielo,1)
					font-size .6em
					font-weight 300
					letter-spacing 1px
					margin 2px 1em 0 0
					text-transform uppercase
					p
						&:before
							color rgba(cielo,.8)
							content '\e600'
							font-family icono
							margin 0 5px 0 0 
			.previ
				border-top 1px solid #08849b
				color rgba(#08849b,1)
			.festes
				border-top 1px solid rosa
				color rgba(rosa,1)
			.posterior
				border-top 1px solid #008b63
				color rgba(#008b63,1)
			.religios
				border-top 1px solid #da8e00
				color rgba(#da8e00,1)
			.preExtender
				height 35px
				position relative
				z-index 3
				p
					background rgba(white,.9)
					border-radius 0 0 15px 0
					box-shadow 0 2px 0 rgba(black,.18)
					font-size .60em
					font-style italic
					font-weight 900
					margin 6px 0 0 0 
					padding 10px 10px
					text-transform uppercase
				.girar
					.extender
						display block
						margin -30px 0 0 0
						text-align center
						transform scale(2.5, .8)
						-ms-transform scale(2.5, .8)
						-webkit-transform scale(2.5, .8)
						&:before
							color rgba(cielo,.3)
							content '\e603'
							font-family icono
			&:first-child
				margin-top 10px
	.llistaInvisible
		display none
	.anarInici
		display block
		text-align center
		.boto
			background rgba(white,.1)
			border-radius 50%
			color rgba(white,.5)
			display inline-block
			height 50px
			line-height 2.8em
			margin 1em 0 0 0 
			width 50px
			transform rotateZ(180deg)
			-ms-transform rotateZ(180deg)
			-webkit-transform rotateZ(180deg)
			&:after
				content '\e603'
				font-family icono
				font-size 1.2em
.bg 
	background #1f4567
	height 100%
	left 0
	position fixed
	top 0
	width 100%
	z-index -50
.bgEvento
	background #283148
	bottom 0
	box-shadow 0 2px 0 rgba(black,.2)
	height 255px
	left 0 
	position absolute
	right 0
	top 425px
	z-index 5
.bgHeader
