   		}
	height 445px
	left 0
	position absolute 
	right 0
	top 0
	width 100%
	z-index 0
.efectos
	height 600px
	width 100%
	top 0px
	overflow-x hidden
	overflow-y hidden
	position absolute
	.dia
		.moverDestellos
		.nubes
		.rayos
		.sol
			display none
	.nit
		.contenedorCastell
			display block
			margin 0 auto
			position relative
			width 150px
			z-index 4
			.castell
				background-image url(../img/castell.png)
				background-repeat no-repeat
				background-size 243px auto
				height 119px
				left -50px
				position absolute
				top 305px
				width 243px
			.castellFosc
				background-image url(../img/castellFosc.png)
				background-repeat no-repeat
				background-size 243px auto
				height 119px
				left -50px
				position absolute
				top 305px
				width 243px
				animation animacionFosc 15s infinite
				-webkit-animation animacionFosc 15s infinite
		.estrellas
			height 445px
			left 0 
			position relative
			right 0
			top 0
			z-index 1
			.moverFugaz
				display block
				position absolute 
				z-index 1
				right -10px
				top -10px
				transform rotateZ(-50deg)
				-ms-transform rotateZ(-50deg)
				-webkit-transform rotateZ(-50deg)
				.fugaz
					background white
					border-radius 50%
					height 1px
					width 35px
					-webkit-filter brightness(3)
					animation animacionFugaz 16s infinite
					-webkit-animation animacionFugaz 16s infinite
			.moverConstelacion
				height 445px
				overflow-x auto
				overflow-y hidden
				position relative
				z-index 1
				.constelacion
					background-image url(../img/estrellas.png)
					background-repeat no-repeat
					background-size 900px auto
					// border 2px solid white
					height 900px
					left -250px
					position absolute
					top 0px
					width 900px
					opacity 1
					-webkit-filter brightness(1.8)
					animation animacionConstelacion 320s infinite
					-webkit-animation animacionConstelacion 320s infinite
		.degradadoA
			@css{
				background: -moz-linear-gradient(top,  rgba(84,84,84,0) 0%, rgba(255,255,255,0.07) 49%, rgba(255,255,255,0.51) 80%, rgba(159,91,37,0.8) 100%);
				background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(84,84,84,0)), color-stop(49%,rgba(255,255,255,0.07)), color-stop(80%,rgba(255,255,255,0.51)), color-stop(100%,rgba(159,91,37,0.8)));
				background: -webkit-linear-gradient(top,  rgba(84,84,84,0) 0%,rgba(255,255,255,0.07) 49%,rgba(255,255,255,0.51) 80%,rgba(159,91,37,0.8) 100%);
				background: -o-linear-gradient(top,  rgba(84,84,84,0) 0%,rgba(255,255,255,0.07) 49%,rgba(255,255,255,0.51) 80%,rgba(159,91,37,0.8) 100%);
				background: -ms-linear-gradient(top,  rgba(84,84,84,0) 0%,rgba(255,255,255,0.07) 49%,rgba(255,255,255,0.51) 80%,rgba(159,91,37,0.8) 100%);
				background: linear-gradient(to bottom,  rgba(84,84,84,0) 0%,rgba(255,255,255,0.07) 49%,rgba(255,255,255,0.51) 80%,rgba(159,91,37,0.8) 100%);
				filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00545454', endColorstr='#cc9f5b25',GradientType=0 );
				}
			opacity .2
			display block
			width 100%
			height 445px
			top 0px
			position absolute
			z-index 3	
		.degradadoB
			@css{
				background: -moz-linear-gradient(top,  rgba(30,87,153,0) 0%, rgba(244,118,0,1) 100%);
