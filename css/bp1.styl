				background: linear-gradient(to bottom,  rgba(30,87,153,0) 0%,rgba(244,118,0,1) 100%);
				filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#001e5799', endColorstr='#f47600',GradientType=0 );
				}
			opacity .12
			display block
			width 100%
			height 145px
			top 300px
			position absolute
			z-index 3		
.invisible
.menu
	background cielo
	border-radius 5px
	bottom 0
	color white
	height auto
	left 0
	margin 0 auto .4em auto
	position fixed
	right 0
	width 97%
	z-index 20
	animation animacionMenu 1s 1
	-webkit-animation animacionMenu 1s 1
	.botones
		.botonMenu
			color #283148
			display inline-block
			line-height 2em
			text-align center
			width 32%
			a
				color #283148
				text-decoration none
			.compartir
				font-size 1.2em
				&:after
					content '\e602'
					font-family icono
					font-size .85em
				&:active
					color rgba(#283148,.3)
			.mapa
				font-size 1.15em
				&:after
					content '\e606'
					font-family icono
					font-size 1em
					vertical-align bottom
				&:active
					color rgba(white,.3)
			.menuResizer
				font-weight 400
				&:active
					color rgba(#283148,.3)
				.aumentar
					&:after
						content 'A'
						font-size 1em
				.disminuir
					&:after
						content 'A'
						font-size .6em
	.resizer
		background rgba(black,.2)
		border-top 1px solid rgba(#283148,.4)
		box-shadow 0px 1px 0px rgba(white,.1)
		color rgba(#283148,.8)
		display none
		padding .4em 0
		.botonResizer
			display inline-block
			font-weight 400
			text-align center
			width 49%
			.disminuir
				border-right 1px solid rgba(#283148,.4)
				box-shadow 1px 0px 0px rgba(white,.1)
				&:after
					content 'A'
					font-size .7em
				&:active
					color rgba(#283148,.3)
			.aumentar
				&:after
					content 'A'
					font-size 1.2em
				&:active
						color rgba(#283148,.3)
	.social
		background rgba(black,.2)
		border-top 1px solid rgba(#283148,.45)
		display none
		padding .8em 0 .6em 0
		.botonSocial
			display inline-block
			font-weight 400
			text-align center
			width 49%
			.facebook
				display block
			.twitter
				display block
				margin 0 0 0 1.5em
.facebook
.twitter
	display none
.patron
	background-image url(../img/patronNegro.png)
	background-repeat repeat
	background-size 200px auto
	bottom 0
	height 100%
	left 0
	opacity .2
	position fixed
	right 0
	top 0
	width 100%
	z-index -5
@keyframes animacionConstelacion
	0%
		transform rotateZ(0deg)
		-ms-transform rotateZ(0deg)
		-webkit-transform rotateZ(0deg)

	50%
		transform rotateZ(360deg)
		-ms-transform rotateZ(360deg)
		-webkit-transform rotateZ(360deg)

	100%
		transform rotateZ(0deg)
		-ms-transform rotateZ(0deg)
		-webkit-transform rotateZ(0deg)
@keyframes animacionDia 	
	0% 
		opacity 0
	50%
		opacity 1
	75%
		opacity 1
	100% 
		opacity 1
@keyframes animacionFugaz 
	0%
		opacity 0
		transform translate(0px)
		-ms-transform translate(0px)
		-webkit-transform translate(0px)
	1%
		opacity .3
	15%
		opacity 0
	30%
		opacity 0
		transform translate(-900px)
		-ms-transform translate(-900px)
		-webkit-transform translate(-900px)
	35%
		opacity 0
	100%
		opacity 0
		transform translate(0px)
		-ms-transform translate(0px)
		-webkit-transform translate(0px)
@keyframes animacionFosc
	0% 
		opacity 1
	50%
		opacity .1
	75%
		opacity 1
	100% 
		opacity 1
@keyframes animacionMenu 
	0% 
		opacity 0
	100% 
		opacity 1
@keyframes animacionMes 
	0% 
		opacity 0
	50%
		opacity 0
	75%
		opacity 0
	100% 
		opacity 1
@keyframes animacionNumero 
	0% 
		opacity 0
	50%
		opacity 0
	75%
		opacity 1
	100% 
		opacity 1
























