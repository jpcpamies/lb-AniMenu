@keyframes animacionDestellos 
	0%
		transform rotateZ(-18deg)
		-ms-transform rotateZ(-18deg)
		-webkit-transform rotateZ(-18deg)
	50%
		transform rotateZ(-33deg)
		-ms-transform rotateZ(-33deg)
		-webkit-transform rotateZ(-33deg)
	100%
		transform rotateZ(-18deg)
		-ms-transform rotateZ(-18deg)
		-webkit-transform rotateZ(-18deg)
@keyframes animacionMoverDestellos
	0%
		transform translate(0px)
		-ms-transform translate(0px)
		-webkit-transform translate(0px)
	50%
		transform translate(50px, -50px)
		-ms-transform translate(50px, -50px)
		-webkit-transform translate(50px, -50px)

	100%
		transform translate(0px)
		-ms-transform translate(0px)
		-webkit-transform translate(0px)
@keyframes animacionDestello1
	0%
		transform translate(0, 0px)
		-ms-transform translate(0, 0px)
		-webkit-transform translate(0, 0px)
		opacity .2
	25%
		transform translate(0, 90px)
		-ms-transform translate(0, 90px)
		-webkit-transform translate(0, 90px)
		opacity .4
	50%
		transform translate(0, 150px)
		-ms-transform translate(0, 150px)
		-webkit-transform translate(0, 150px)
		opacity .8
	75%
		transform translate(0, 90px)
		-ms-transform translate(0, 90px)
		-webkit-transform translate(0, 90px)
		opacity 1
	100%
		transform translate(0, 0px)
		-ms-transform translate(0, 0px)
		-webkit-transform translate(0, 0px)
		opacity .2
@keyframes animacionDestello2
	0%
		transform translate(0, 0px)
		-ms-transform translate(0, 0px)
		-webkit-transform translate(0, 0px)
		opacity .2
	25%
		transform translate(0, 80px)
		-ms-transform translate(0, 80px)
		-webkit-transform translate(0, 80px)
		opacity .5
	50%
		transform translate(0, 130px)
		-ms-transform translate(0, 130px)
		-webkit-transform translate(0, 130px)
		opacity .7
	75%
		transform translate(0, 90px)
		-ms-transform translate(0, 90px)
		-webkit-transform translate(0, 90px)
		opacity 1
	100%
		transform translate(0, 0px)
		-ms-transform translate(0, 0px)
		-webkit-transform translate(0, 0px)
		opacity .2
@keyframes animacionDestello3
	0%
		transform translate(0, 0px)
		-ms-transform translate(0, 0px)
		-webkit-transform translate(0, 0px)
		opacity .6
	25%
		transform translate(0, 65px)
		-ms-transform translate(0, 65px)
		-webkit-transform translate(0, 65px)
		opacity 1
	50%
		transform translate(0, 160px)
		-ms-transform translate(0, 160px)
		-webkit-transform translate(0, 160px)
		opacity .3
	75%
