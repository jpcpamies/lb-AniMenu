.patron
	position fixed
	top 0
	bottom 0
	right 0
	left 0
	width 100%
	height 100%
	background-image url(../img/patron.png)
	background-repeat repeat
	background-size 408px auto
	z-index -5
	opacity .05

@keyframes animacionConstelacion
	0%
		transform rotateZ(0deg)
		-ms-transform rotateZ(0deg)
		-webkit-transform rotateZ(0deg)

	50%
		transform rotateZ(120deg)
		-ms-transform rotateZ(120deg)
		-webkit-transform rotateZ(120deg)

	100%
		transform rotateZ(0deg)
		-ms-transform rotateZ(0deg)
		-webkit-transform rotateZ(0deg)
@keyframes animacionDia 	
	0% 
		opacity 0
	50%
		opacity 1
	75%
		opacity 1
	100% 
		opacity 1
@keyframes animacionFugaz 
	0%
		opacity 0
		transform translate(0px)
		-ms-transform translate(0px)
		-webkit-transform translate(0px)
	1%
		opacity .3
	15%
		opacity 0
	30%
		opacity 0
		transform translate(-900px)
		-ms-transform translate(-900px)
		-webkit-transform translate(-900px)
	35%
		opacity 0
	100%
		opacity 0
		transform translate(0px)
		-ms-transform translate(0px)
		-webkit-transform translate(0px)
@keyframes animacionFosc
	0% 
		opacity 1
	50%
		opacity .1
	75%
		opacity 1
	100% 
		opacity 1
@keyframes animacionMes 
	0% 
		opacity 0
	50%
		opacity 0
	75%
		opacity 0
	100% 
		opacity 1
@keyframes animacionNumero 
	0% 
		opacity 0
	50%
		opacity 0
	75%
		opacity 1
	100% 
		opacity 1
























