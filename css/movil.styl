		height auto
		overflow-x auto
		overflow-y hidden
		padding 10px 20px 0 10px 
		white-space nowrap
		width 100%
		position relative
		z-index 15
		ul
			text-align center
			.dia 
				border-radius 50%
				display inline-block
				height 65px 
				margin 0
				position relative
				width 65px
				p
					border-radius 50%
