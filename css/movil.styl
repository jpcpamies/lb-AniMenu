	.calendari
		// background rgba(red,.3)
		color white
		display block
		height 445px
		margin 0 auto
		padding 3em 0 0 0 
		position relative
		width 100%
		z-index 10
		.dia
			font-size 1em
			font-weight 300
			text-align center
			text-shadow 0 1px 3px rgba(black,.4)
			animation animacionDia 3s 1
			-webkit-animation animacionDia 3s 1
		.numero
			font-size 4em
			font-weight 100
			margin -.1em 0
			text-align center
			text-shadow 0 1px 3px rgba(black,.4)
			animation animacionNumero 3s 1
			-webkit-animation animacionNumero 3s 1
		.mes
			font-size 1em
			font-weight 300
			text-align center
			text-shadow 0 1px 3px rgba(black,.4)
			animation animacionMes 3s 1
			-webkit-animation animacionMes 3s 1
	.eventActual
		// background rgba(black,.3)
		// border-top 1px solid rgba(white,.2)
		color white
		display block
		font-size .9em
		font-weight 100
		height auto
		padding .7em 1em 1.2em 1em
		position relative
		z-index 5
		.contingutEventActual
			animation animacionLlista 3s 1
			-webkit-animation animacionLlista 3s 1
			.event
				color rgba(white,1)
				display block
				font-size .9em
				font-weight 300
				letter-spacing .055em
				line-height 1.2em
				margin .7em auto 0 auto
				padding .7em 0 0 0
				text-align center
				width 90%
				.lloc
					color rgba(white,.5)
					font-size .7em
					letter-spacing .13em
					margin .5em 0 0 0 
					text-transform uppercase
					p
						&:after
							content '\e605'
							font-family icono
			.hora
				color white
				display block
				font-size .9em
				font-weight 300
				margin .6em auto 0 auto
				text-align center
				width 100px
				// p
				// 	&:after
				// 		content 'h'
		.titol
			// background rgba(white,.5)
			border-radius 10px
			color rgba(#c0fff2,1)
			font-size 1em
			font-weight 400
			left 10px
			top 20px
			// line-height 2em
			padding 0 .7em
			position absolute
			// text-transform uppercase
			span
				position absolute
				left 50px
				top 0px
				transform rotateZ(180deg)
				-ms-transform rotateZ(180deg)
				-webkit-transform rotateZ(180deg)
				&:after
					content '\e603'
					font-family icono
section
	margin 0 0 60px 0
	display block
	position relative
	overflow-x hidden
	.llistaVisible
		animation animacionLlista .3s 1
		-webkit-animation animacionLlista .3s 1
		padding 1em 0 1em 0
		.event
			margin  10px 10px
			.detallEvent
				background rgba(white,.3)
				border-radius 10px 10px 0 0 
				margin .5em 0 0 0 
				color rgba(black,.8)
				display block
				height 30px
				.quan
					.data
						p
							color rgba(#c0fff2,.5)
							font-size .70em
							font-weight 300
							left 70px
							position relative
							top -10px
					.hora
						color rgba(#c0fff2,1)
						font-size .7em
						font-weight 400
						letter-spacing 2px
						position relative
						top 7px
						left 10px
						p
							&:after
								content 'h'
			.Event
					background rgba(white,.4)
					border-radius 0 0 10px 10px
					color rgba(#c0fff2,1)
					display block
					font-size .9em
					font-weight 400
					// margin 1px 0 0 0 
					padding .7em .5em .8em .5em
					position relative
					.detall
						display none
						width 95%
						font-size .8em
						font-weight 300
						margin 1em 0 1em 1em 
						p
							&:after
								display none
					.expandir
						position absolute 
						right .2em
						top .7em
						transform rotateZ(90deg)
						-ms-transform rotateZ(90deg)
						-webkit-transform rotateZ(90deg)
						&:after
							content '\e60b'
							color rgba(black,.3)
							font-family icono
							font-size 1em
					.lloc
						color rgba(#c0fff2,1)
						font-size .6em
						font-weight 300
						letter-spacing 1px
						margin .5em 1em 0 0
						text-transform uppercase
						p
							&:before
								content '\e605'
								font-family icono
								color rgba(#c0fff2,1)
		.anarInici
			display block
			text-align center
			.boto
				background rgba(white,.1)
				border-radius 50%
				color rgba(white,.5)
				display inline-block
				height 50px
				line-height 2.5em
				margin 1em 0 0 0 
				width 50px
				&:after
					content '\e604'
					font-family icono
					font-size 1.5em
	.llistaInvisible
		display none
