						color rgba(black,.7)
						font-weight 400
				.centrarMes
					margin 0 0 0 .35em
				.mes
					color rgba(white,.8)
					font-size .7em
					font-weight 300
					left 25%
					letter-spacing 0em
					position absolute
					top 55px
				.nomDia
					color rgba(white,.8)
					font-size .7em
					font-weight 300
					left 30%
					position absolute
					top -5px
				&:last-child
