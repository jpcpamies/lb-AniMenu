
header
	width 100%
	overflow-x hidden
	overflow-y hidden
	position relative	
	.bgHeader
		@css {
			background: rgb(6,64,112);
			background: -moz-linear-gradient(45deg,  rgba(6,64,112,1) 0%, rgba(3,30,57,1) 53%, rgba(6,13,13,1) 87%);
